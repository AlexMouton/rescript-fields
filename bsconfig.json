{
  "name": "@nobleai/rescript-fields",
  "jsx": {"version": 4, "mode": "classic"},
  "suffix": ".bs.js",
  "sources": [{
    "dir": "src",
    "subdirs": true
  },{
    "dir": "test",
    "subdirs": true,
    "type" : "dev"
  },{
    "dir": "examples",
    "subdirs": true,
    "type" : "dev"
  }],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "bsc-flags": [
    "-open Prelude"
  ],
  "bs-dependencies": [
    "@rescript/react",
    "@nobleai/rescript-rxjs",
    "@nobleai/rescript-prelude"
  ],
  "bs-dev-dependencies": [
    "@nobleai/rescript-vitest",
    "@nobleai/rescript-lodash",
    "@nobleai/rescript-testing-library"
  ]
}
